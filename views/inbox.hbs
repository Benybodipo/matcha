<div class="menu">
	<ul class="clearfix">
		<li><a href="" class="clearfix">Matches<i class="fas fa-heart"></i></a></li>
		<li><a href="" class="clearfix">Fans<i class="fas fa-star"></i></a></li>
		<li><a href="" class="clearfix">Visitors<i class="far fa-eye"></i></a></li>
	</ul>
</div>
<div id="contacts-list">
	{{#each users}}
		<div class="contact clearfix" data-userid="{{_id}}">
			<img src="{{images}}" alt="">
			<span class="name"><b>{{username}}</b></span>
			<small>30km away</small>
		</div>
	{{/each}}
</div>
<div id="messages-list">
	<!-- HERE GOES THE CHATS  -->
</div>
<div id="chat" class="clearfix">
	<div id="chat-box" data-receiver="">
		<div id="header" class="header clearfix">
			<div class="user-info clearfix">
				<img src="/img/face5.jpg" alt=""><strong>Jessica</strong>
			</div>
			<div class="icons">
				<i class="fas fa-info-circle"></i>
				<i class="fas fa-expand-arrows-alt"></i>
				<i class="far fa-times-circle"></i>
			</div>
		</div>
		<div class="messages clearfix">
			<!-- HERE GOES ALL THE MESSAGES OF THE CHAT -->
			<hr>
		</div>
		<div class="text">
			<textarea name="name" placeholder="Type your message.." id="message"></textarea>
		</div>
	</div>
</div>
